<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zszd.ai.dao.EmployeeDao">
	<select id="updateEmployeeWorkPoint" resultType="Integer" parameterType="Employee">
		update employee set workpoints = #{workpoints} where id =#{id}
	</select>

</mapper>