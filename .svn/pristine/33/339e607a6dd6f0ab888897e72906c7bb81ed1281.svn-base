<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zszd.ai.dao.ResourceDao">
	<select id="queryPassedResourceInfoByType" resultType="Resource">
		select * from resource where state = 1 and type = #{type}
	</select>
	
	<select id="queryAllResourceInfoByType" resultType="Resource">
		select * from resource where type = #{type}
	</select>
	
	<insert id="insertResourceRecord" parameterType="Resource">
		insert into resource (type,name,path,createBy) values (#{type},#{name},#{path},#{createBy})
	</insert>
</mapper>